        <input  name="B" to="g1.B"/>
        <output name="Y" from="g1"/>
    </module>

    <module name="xor-2">
        <gate type="XOR" name="g1"/>
        <input  name="A" to="g1.A"/>
        <input  name="B" to="g1.B"/>
        <output name="Y" from="g1"/>
    </module>

    <module name="xnor-2">
        <gate type="XNOR" name="g1"/>
        <input  name="A" to="g1.A"/>
        <input  name="B" to="g1.B"/>
        <output name="Y" from="g1"/>
    </module>

</root>




<root>
    <module name="complex-xor">
        <gate type="OR" name="o1"/>
        <gate type="AND" name="a1"/>
        <gate type="AND" name="a2"/>
        <gate type="NOT" name="n1"/>
        <gate type="NOT" name="n2"/>

        <signal from="n1" to="a1.B"/>
        <signal from="n2" to="a2.A"/>
        <signal from="a1" to="o1.A"/>
        <signal from="a2" to="o1.B"/>

        <input name="A" to="a1.A:n2.A"/>
        <input name="B" to="a2.B:n1.A"/>

        <output name="Y" from="o1"/>
    </module>
</root>
